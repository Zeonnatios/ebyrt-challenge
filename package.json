    "http-status-codes": "^2.2.0",
    "joi": "^17.6.0",
    "mongodb": "^4.1.3"
  },
  "devDependencies": {
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "eslint": "6.8.0",
    "eslint-config-trybe-backend": "^1.0.4",
    "mocha": "^9.2.0",
    "mongodb-memory-server": "^6.9.6",
    "nodemon": "^2.0.14",
    "nyc": "^15.1.0",
    "sinon": "^13.0.1"
  }
